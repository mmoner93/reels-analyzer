# Port Configuration for Reels Analyzer
# These ports are used to access the services from your host machine

# Frontend port (access the web UI)
FRONTEND_PORT=3000

# API port (access the REST API)
API_PORT=8000

# Redis port (for development/debugging)
REDIS_PORT=6379

# Environment
NODE_ENV=production
PYTHONUNBUFFERED=1

# Logging
LOG_LEVEL=info
